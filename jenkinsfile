node {
  
  stage('GIT CheckOut') {
    git 'https://github.com/ruchi672/projectRepository.git'
  }

  stage('Maven-Clean') {
    sh label: '', script: 'mvn clean'
  }

  stage('Maven-Compile') {
    sh label: '', script: 'mvn compile'
  }
  
  stage('Maven-Test') {
    sh label: '', script: 'mvn test'
  }


 stage('Sonar Analysis') {
   withSonarQubeEnv('SonarQube') {
        sh "mvn sonar:sonar"
    }
  }
  /*
